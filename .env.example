SERVICE_NAME=axion
ENV=development
USER_PORT=5111

# Redis
REDIS_URI=redis://127.0.0.1:6379
CACHE_REDIS=redis://127.0.0.1:6379
CACHE_PREFIX=axion:ch
CORTEX_REDIS=redis://127.0.0.1:6379
CORTEX_PREFIX=axion
CORTEX_TYPE=axion

# Security secrets
SHORT_TOKEN_SECRET=replace_with_secure_short_secret
ACCESS_TOKEN_EXPIRES_IN=12h
ACCESS_TOKEN_KEYS=
ACCESS_TOKEN_ACTIVE_KID=
TOKEN_REVOKE_TTL_SEC=604800
PASSWORD_SALT_ROUNDS=10

# API security
API_RATE_LIMIT_MAX=120
API_RATE_LIMIT_WINDOW_SEC=60
RATE_LIMIT_FAIL_OPEN=false
AUTH_LOGIN_MAX_FAILURES=5
AUTH_LOGIN_WINDOW_SEC=900
AUTH_LOGIN_LOCK_SEC=900
CORS_ORIGINS=http://localhost:3000
CORS_ALLOW_ALL=true
ENABLE_DYNAMIC_API=false
REQUEST_MAX_DEPTH=8
REQUEST_MAX_KEYS=2000
